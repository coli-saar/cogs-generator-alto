Rel_clause -> r{{ cnt.next() }}(Rel_pron, V_inf_taking, INF, V_inf)[0.1]
[english] *(*(?1, *(?2, ?3)),?4)
[semantics] xcomp(?2, ?4)

Rel_clause -> r{{ cnt.next() }}(Rel_pron, V_dat, NP_inanimate_dobj, PP_iobj) [0.1]
[english] *(*(*(?1, ?2), ?3),?4)
[semantics] recipient(theme(?2, ?3), ?4)

Rel_clause -> r{{ cnt.next() }}(Rel_pron, V_dat, NP_animate_iobj, NP_inanimate_dobj) [0.1]
[english] *(*(*(?1, ?2), ?3),?4)
[semantics] theme(recipient(?2, ?3), ?4)

{# allows to generate CP phase in the case of RC modifying object:
 - restrictions: no embedded RC modifier
 (e.g.  the boy found the guy that said that Emma froze a cat that slept)
 - the boy found the guy that said that Emma froze a cat
 - embedded CP allowed: the boy found the guy that said that Emma believed that
   the cat slept.
  #}
Rel_clause -> r{{ cnt.next() }}(Rel_pron, V_cp_taking, C, S_internal) [0.15]
[english] *(*(*(?1, ?2), ?3),?4)
[semantics] ccomp(?2, ?4)

S_internal -> r{{ cnt.next() }}(NP_animate_nsubj, VP_external) [0.49]
[english] *(?1, ?2)
[semantics] pre_agent(?2, ?1)

S_internal -> r{{ cnt.next() }}(VP_internal) [0.06]
[english] ?1
[semantics] ?1

S_internal -> r{{ cnt.next() }}(NP_inanimate_nsubjpass, VP_passive) [0.375]
