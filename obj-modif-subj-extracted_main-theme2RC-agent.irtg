[semantics] pre_det(?2, ?1)

VP_main_theme_RC_agent -> r{{ cnt.next() }}(V_main_theme_RC_agent, NP_animate_dobj_main, Rel_clause)[1]
[english] *(*(?1, ?2),?3)
[semantics] pre_theme(pre_nmod(?1, ?3),?2)

V_main_theme_RC_agent -> r{{ cnt.next() }}(V_unacc_theme_RC_agent) [0.33]
