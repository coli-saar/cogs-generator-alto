[english]  ?1
[semantics] ?1

V_main_theme_RC_agent -> r{{ cnt.next() }}(V_trans_omissible_theme_RC_agent) [0.34]
[english]  ?1
[semantics] ?1


Rel_clause -> r{{ cnt.next() }}(Rel_pron, V_unerg) [0.1]
[english] *(?1, ?2)
[semantics] ?2

Rel_clause -> r{{ cnt.next() }}(Rel_pron, V_unacc, NP_dobj) [0.1]
[english] *(*(?1, ?2),?3)
[semantics] theme(?2, ?3)

Rel_clause -> r{{ cnt.next() }}(Rel_pron, V_trans_omissible) [0.1]
