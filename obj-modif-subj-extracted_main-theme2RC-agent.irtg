{## Subsets of main grammar that only generate
subject-extracted clauses that modify NP in object position
argument sharing: main theme --> RC agent
##}

S! -> r{{ cnt.next() }}(NP_animate_nsubj_main, VP_main_theme_RC_agent) [1]
[english] *(?1, ?2)
[semantics] pre_agent(?2,?1)

NP_animate_nsubj_main -> r{{ cnt.next() }}(Det, N_common_animate_nsubj) [0.5]
[english] *(?1, ?2)
[semantics] pre_det(?2, ?1)

NP_animate_nsubj_main -> r{{ cnt.next() }}(N_prop_nsubj) [0.5]
[english] ?1
[semantics] ?1

NP_animate_dobj_main -> r{{ cnt.next() }}(Det, N_common_animate_dobj) [1]
